{
  "name": "@rombotics/template-library-js",
  "version": "1.0.204",
  "description": "A template TypeScript library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push origin HEAD:master && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ROMBOTics/template-library-js.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "Template",
    "Library",
    "Javascript"
  ],
  "license": "UNLICENSED",
  "author": "Christian Nuss",
  "bugs": {
    "url": "https://github.com/ROMBOTics/template-library-js/issues"
  },
  "homepage": "https://github.com/ROMBOTics/template-library-js#readme",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
